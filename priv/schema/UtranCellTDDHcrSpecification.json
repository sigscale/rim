{
	"$id": "http://sigscale.org/schema/UtranCellTDDHcrSpecification",
	"title": "UTRAN TDD HCR Cell Specification",
	"description": "UMTS Time Division Duplex (TDD) high chip rate (HCR) radio resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "UtranCellTDDSpecification"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"type": "array",
					"items": {
						"oneOf": [
							{
								"$ref": "UtranGenericCellSpecification#/definitions/ResourceSpecCharacteristic"
							},
							{
								"$ref": "UtranCellTDDSpecification#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"schPower",
			                        "temporaryOffset1",
			                        "syncCase",
			                        "timeSlotForSch",
			                        "schTimeSlot",
			                        "timeSlotHCRList"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
