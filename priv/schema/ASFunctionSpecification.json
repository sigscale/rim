							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"userLabel",
											"vnfParametersList",
											"linkList",
											"ASFunctionOptionallyContainedNrmClass",
											"VsDataContainer"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
