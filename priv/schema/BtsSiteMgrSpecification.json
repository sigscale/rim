{
	"$id": "http://sigscale.org/schema/BtsSiteMgrSpecification",
	"title": "GSM BTS Specification",
	"description": "GSM Base Transceiver Station (BTS) resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunctionSpecification"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"userLabel",
											"vnfParametersList",
											"latitude",
											"longitude",
											"operationalState",
											"GsmCell",
											"VsDataContainer",
											"InterRatEsPolicies"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
