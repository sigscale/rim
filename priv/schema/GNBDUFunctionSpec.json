{
	"$id": "http://sigscale.org/schema/GNBDUFunctionSpec",
	"title": "NR gNB-DU Specification",
	"description": "NR gNB Distributed Unit (DU) resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunctionSpec"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"id",
											"userLabel",
											"vnfParametersList",
											"peeParametersList",
											"gnbId",
											"gnbDuId",
											"gnbDuName",
											"gnbIdLength"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
