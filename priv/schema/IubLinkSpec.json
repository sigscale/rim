{
	"$id": "http://sigscale.org/schema/IubLinkSpec",
	"title": "UTRAN IUBLINK Specification",
	"description": "UTRAN Iub Link resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunctionSpec"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"type": "array",
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"id",
											"userLabel",
											"vnfParametersList",
											"iubLinkUtranCell",
											"iubLinkATMChannelTerminationPoint",
											"iubLinkNodeBFunction"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
