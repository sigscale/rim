{
	"$id": "http://sigscale.org/schema/SgsnFunctionSpec",
	"title": "Core SGSN Specification",
	"description": "Core Serving GPRS Support Node (SGSN) resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunctionSpec"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"userLabel",
											"vnfParametersList",
											"mccList",
											"mncList",
											"lacList",
											"sacList",
											"sgsnId",
											"sgsnFunctionGsmCell",
											"sgsnFunctionExternalGsmCell",
											"sgsnFunctionSgsnPool",
											"nriList",
											"proceduralStatus",
											"defaultMsc",
											"GbLink",
											"IupsLink",
											"VsDataContainer"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
