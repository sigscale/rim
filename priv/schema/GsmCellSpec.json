{
	"$id": "http://sigscale.org/schema/GsmCellSpec",
	"title": "GERAN Cell Specification",
	"description": "GERAN radio resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunctionSpec"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"id",
											"userLabel",
											"vnfParametersList",
											"cellIdentity",
											"cellAllocation",
											"ncc",
											"bcc",
											"lac",
											"mcc",
											"mnc",
											"rac",
											"racc",
											"tsc",
											"rxLevAccessMin",
											"msTxPwrMaxCCH",
											"rfHoppingEnabled",
											"hoppingSequenceList",
											"plmnPermitted"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
