{
	"$id": "http://sigscale.org/schema/ENBFunction",
	"title": "EUTRAN ENB Specification",
	"description": "LTE Evolved Node B (ENB) resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunctionSpec"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"id",
											"userLabel",
											"vnfParametersList",
											"peeParametersList",
											"intraANRSwitch",
											"iRATANRSwitch",
											"eNBId",
											"x2BlackList",
											"x2WhiteList",
											"x2HOBlackList",
											"x2IpAddressList",
											"tceIDMappingInfoList",
											"sharNetTceMappingInfoList",
											"netListeningRSForRIBS"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
