{
	"$id": "http://sigscale.org/schema/ManagedFunction",
	"title": "Managed Function",
	"description": "A telecommunication function either realized by software running on dedicated hardware or realized by software running on NFVI. (3GPP TS 28.622)",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunction"
		},
		{
			"type": "object",
			"properties": {
				"characteristic": {
					"type": "array",
					"items": {
						"allOf": [
							{
								"$ref": "resourceInventoryManagement#/definitions/ResourceCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"userLabel",
											"vnfParametersList",
											"peeParametersList",
											"priorityLabel"
										]
									}
								}
							}
						]
					}
				},
				"vsDataContainer": {
					"$ref": "genericNrm#/definitions/VsDataContainerList"
				}
			}
		}
	]
}
