							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"id",
											"aEnd",
											"linkType",
											"protocolName",
											"protocolVersion",
											"userLabel",
											"zEnd"
										]
									}
								}
							}
						]
					}
