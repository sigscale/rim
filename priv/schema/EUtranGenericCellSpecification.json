{
	"$id": "http://sigscale.org/schema/EUtranGenericCellSpecification",
	"title": "EUTRAN Generic Cell Specification",
	"description": "LTE radio cell common properties resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunctionSpecification"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"type": "array",
					"items": {
						"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
					}
				}
			}
		}
	],
	"definitions": {
		"ResourceSpecCharacteristic": {
			"type": "object",
			"required": [
				"name"
			],
			"properties": {
				"name": {
					"enum": [
						"userLabel",
						"vnfParametersList",
						"cId",
						"localCellId",
						"maximumTransmissionPower",
						"cellMode",
						"pichPower",
						"pchPower",
						"fachPower",
						"lac",
						"rac",
						"sac",
						"uraList",
						"utranCellIubLink",
						"relatedAntennaList",
						"relatedTmaList",
						"operationalState",
						"hsFlag",
						"hsEnable",
						"numOfHspdschs",
						"numOfHsscchs",
						"frameOffset",
						"cellIndividualOffset",
						"hcsPrio",
						"maximumAllowedUlTxPower",
						"snaInformation",
						"qrxlevMin",
						"deltaQrxlevmin",
						"qhcs",
						"penaltyTime",
						"referenceTimeDifferenceToCell",
						"readSFNIndicator",
						"nsPlmnIdList",
						"restrictionStateIndicator",
						"dpcModechangeSupportIndicator",
						"relatedSectorEquipment",
						"utranRelation",
						"gsmRelation",
						"vsDataContainer"
					]
				}
			}
		}
	}
}
