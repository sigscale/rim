{
	"$id": "http://sigscale.org/schema/UtranCellFDDSpec",
	"title": "UTRAN FDD Cell Specification",
	"description": "UMTS Frequency Division Duplex (FDD) radio resource function specification",
	"allOf": [
		{
			"$ref": "resourceCatalogManagement#/definitions/ResourceFunctionSpecification"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"type": "array",
					"items": {
						"oneOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecificationCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"id",
											"uarfcnUl",
											"uarfcnDl",
											"primaryScramblingCode",
											"primaryCpichPower",
											"primarySchPower",
											"secondarySchPower",
											"bchPower",
											"aichPower",
											"qqualMin",
											"cellCapabilityContainerFDD",
											"txDiversityIndicator",
											"temporaryOffset1",
											"temporaryOffset2",
											"sttdSupportIndicator",
											"closedLoopModelSupportIndicator"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
