					"type": "array",
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecificationCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"fdn",
											"className",
											"cLASSNAME",
											"mOIndex",
											"name",
											"neID"
										]
