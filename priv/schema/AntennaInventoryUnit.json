{
	"$id": "http://sigscale.org/schema/AntennaInventoryUnit",
	"title": "IM Antenna Inventory Unit",
	"description": "IM Antenna Inventory Unit resource function",
	"type": "object",
	"allOf": [
		{
			"$ref": "InventoryUnit"
		},
		{
			"type": "object",
			"properties": {
				"characteristic": {
					"type": "array",
					"items": {
						"allOf": [
							{
								"$ref": "resourceInventoryManagement#/definitions/ResourceCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"maxTiltValue",
											"minTiltValue",
											"mechanicalOffset",
											"baseElevation",
											"latitude",
											"longitude",
											"patternLabel"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
