							{
								"$ref": "resourceInventoryManagement#/definitions/ResourceCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"uarfcn",
											"cellParameterId",
											"primaryCcpchPower",
											"cellCapabilityContainerTDD",
											"sctdIndicator",
											"dpchConstantValue"
										]
									}
								}
							}
						]
					}
				}
