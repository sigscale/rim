{
	"$id": "http://sigscale.org/schema/MobileEdgeApplication",
	"title": "UTRAN RNC",
	"description": "MEC Mobile Edge Application resource",
	"type": "object",
	"allOf": [
		{
			"$ref": "ManagedFunction"
		},
		{
			"type": "object",
			"properties": {
				"characteristic": {
					"type": "array",
					"items": {
						"allOf": [
							{
								"$ref": "resourceInventoryManagement#/definitions/ResourceCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"appDId",
											"appName",
											"appProvider",
											"appSoftVersion",
											"appDVersion",
											"appInfoName",
											"appDescription",
											"appState",
											"instantiationState",
											"operationalState",
											"appInstanceId"
										]
									}
								}
							}
						]
					}
				},
				"mobileEdgeApplicationService": {
					"$ref": "genericNrm#/definitions/DnList"
				}
			}
		}
	]
}
