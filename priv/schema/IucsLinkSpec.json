{
	"$id": "http://sigscale.org/schema/IucsLinkSpec",
	"title": "Core IucsLink Specification",
	"description": "Core Iu-cs interface link (connecting an MSCserver to the RNC, BSC and HNB GW) resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "resourceCatalogManagement#/definitions/ResourceFunctionSpecification"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"type": "array",
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecificationCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"id",
											"userLabel",
											"vnfParametersList",
											"connectedRnc",
											"connectedBss",
											"connectedHNBGW"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
