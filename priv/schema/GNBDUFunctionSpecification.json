{
	"$id": "http://sigscale.org/schema/GNBDUFunctionSpecification",
	"title": "NR gNB-DU Specification",
	"description": "NR gNB Distributed Unit (DU) resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunctionSpec"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"userLabel",
											"vnfParametersList",
											"peeParametersList",
											"gnbId",
											"gnbDUId",
											"gnbDuName",
											"NRCellDU",
											"EP_F1C",
											"EP_F1U"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
