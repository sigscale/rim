		"MeContextList": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/MeContext"
			}
		},
		"ManagementNode": {
			"allOf": [
				{
					"$ref": "#/definitions/NrmClass"
				},
				{
					"type": "object",
					"properties": {
						"attributes": {
							"type": "object",
							"required": [
								"userLabel",
								"vendorName",
								"locationName",
								"swVersion",
								"userDefinedState"
