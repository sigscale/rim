{
	"title": "Generic NRM",
	"description": "Generic Network Resource Model (NRM)",
	"definitions": {
		"NrmClass": {
			"type": "object",
			"title": "NrmClass",
			"description": "Base type for all NRM class associated objects",
			"required": [
				"id"
			],
			"properties": {
				"id": {
					"type": "string"
				},
				"modifier": {
					"type": "string",
					"enum": [
						"create",
						"delete",
						"update"
					]
				}
			}
		},
		"dn": {
			"type": "string",
			"maxLength": 400
		},
		"dnList": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/dn"
			}
		},
		"linkType": {
			"type": "array",
			"items": {
				"type": "string",
				"enum": [
					"Signalling",
					"Bearer",
					"OAM_AND_P",
					"Other"
				]
			}			
		},
		"linkListType": {
			"type": "array",
			"properties": {
				"dn": {
					"$ref": "#/definitions/dn"
				}
			}
		},
		"managedElementTypeListType": {
			"type": "array",
			"items": {
				"type": "object",
				"required": [
					"managedElementType"
				],
				"properties": {
					"managedElementType": {
						"type": "string"
					}
				}
			}
		},
		"vnfParametersListType": {
			"type": "array",
			"items": {
				"type": "object",
				"required": [
					"vnfInstanceId",
					"autoScalable"
				],
				"properties": {
					"vnfInstanceId": {
						"type": "string"
					},
					"vnfdId": {
						"type": "string"
					},
					"flavourId": {
						"type": "string"
					},
					"autoScalable": {
						"type": "boolean"
					}
				}
			},
			"minItems": 1
		},
		"latitude": {
			"type": "number",
			"minimum": -90.0000,
			"maximum": 90.0000
		},
		"longitude": {
			"type": "number",
			"minimum": -180.0000,
			"maximum": 180.0000
		},
		"peeParametersListType": {
			"type": "array",
			"items": {
				"type": "object",
				"required": [
					"siteIdentification",
					"siteDescription",
					"equipmentType",
					"environmentType",
					"powerInterface"
				],
				"properties": {
					"siteIdentification": {
						"type": "string"
					},
					"siteLatitude": {
						"$ref": "#/definitions/latitude"
					},
					"siteLongitude": {
						"$ref": "#/definitions/longitude"
					},
					"siteDescription": {
						"type": "string"
					},
					"equipmentType": {
						"type": "string"
					},
					"environmentType": {
						"type": "string"
					},
					"powerInterface": {
						"type": "string"
					}
				}
			},
			"minItems": 1
		},
		"SubNetwork": {
			"allOf": [
				{
					"$ref": "#/definitions/NrmClass"
				},
				{
					"type": "object",
					"properties": {
						"attributes": {
							"type": "object",
							"required": [
								"userLabel",
								"userDefinedNetworkType"
							],
							"properties": {
								"dnPrefix": {
									"$ref": "#/definitions/dn"
								},
								"userLabel": {
									"type": "string"
								},
								"userDefinedNetworkType": {
									"type": "string"
								},
								"setOfMcc": {
									"type": "array",
									"items": {
										"type": "integer",
										"minimum": 1,
										"maximum": 999
									}
								}
							}
						},
						"subNetwork": {
							"$ref": "#/definitions/SubNetworkList"
						},
						"managedElement": {
							"$ref": "#/definitions/ManagedElementList"
						},
						"meContext": {
							"$ref": "#/definitions/MeContextList"
						},
						"managementNode": {
							"$ref": "#/definitions/ManagementNodeList"
						},
						"irpAgent": {
							"$ref": "#/definitions/IRPAgentList"
						},
						"subNetworkOptionallyContainedNrmClass": {
							"$ref": "#/definitions/SubNetworkOptionallyContainedNrmClassList"
						},
						"vsDataContainer": {
							"$ref": "#/definitions/VsDataContainerList"
						},
						"esPolicies": {
							"$ref": "sonPolicyNrm#/definitions/ESPoliciesList"
						}
					}
				}
			]
		},
