			"type": "array",
			"items": {
				"$ref": "#/definitions/ManagementNode"
			}
		},
		"IRPAgent": {
			"allOf": [
				{
					"$ref": "#/definitions/NrmClass"
				},
				{
					"type": "object",
					"properties": {
						"attributes": {
							"type": "object",
							"properties": {
								"systemDN": {
									"$ref": "#/definitions/systemDN"
								}
							}
						}
					}
				}
			]
		},
		"IRPAgentList": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/IRPAgent"
			}
		},
		"VsDataContainer": {
			"allOf": [
