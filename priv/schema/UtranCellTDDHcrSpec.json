{
	"$id": "http://sigscale.org/schema/UtranCellTDDHcrSpec",
	"title": "UTRAN TDD HCR Cell Specification",
	"description": "UMTS Time Division Duplex (TDD) high chip rate (HCR) radio resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "UtranCellTDDSpec"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"type": "array",
					"items": {
						"oneOf": [
							{
								"$ref": "UtranGenericCellSpec#/definitions/ResourceSpecCharacteristic"
							},
							{
								"$ref": "UtranCellTDDSpec#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"id",
											"schPower",
											"temporaryOffset1",
											"syncCase",
											"timeSlotForSch",
											"schTimeSlot",
											"timeSlotHCRList"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
