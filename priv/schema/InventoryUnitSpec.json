{
	"$id": "http://sigscale.org/schema/InventoryUnitSpec",
	"title": "IM Inventory Unit Specification",
	"description": "IM Inventory Unit resource function specification",
	"type": "object",
	"allOf": [
		{
			"$ref": "ResourceFunctionSpec"
		},
		{
			"type": "object",
			"properties": {
				"resourceSpecCharacteristic": {
					"type": "array",
					"items": {
						"allOf": [
							{
								"$ref": "resourceCatalogManagement#/definitions/ResourceSpecCharacteristic"
							},
							{
								"type": "object",
								"required": [
									"name"
								],
								"properties": {
									"name": {
										"enum": [
											"id",
											"inventoryUnitType",
											"vendorUnitFamilyType",
											"vendorUnitTypeNumber",
											"vendorName",
											"serialNumber",
											"dateOfManufacture",
											"dateOfLastService",
											"unitPosition",
											"manufacturerData",
											"versionNumber",
											"relatedFunction"
										]
									}
								}
							}
						]
					}
				}
			}
		}
	]
}
